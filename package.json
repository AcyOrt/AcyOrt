{
  "name": "acyort",
  "version": "3.0.0",
  "description": "A Node.js static website framework",
  "bin": {
    "acyort": "./bin/acyort.js"
  },
  "files": [
    "assets",
    "bin",
    "lib"
  ],
  "main": "./lib/index.js",
  "preferGlobal": true,
  "scripts": {
    "test": "cd assets && node ../bin/acyort flow",
    "clean": "cd assets && node ../bin/acyort clean",
    "version": "cd assets && node ../bin/acyort -v",
    "help": "cd assets && node ../bin/acyort",
    "config": "cd assets && node ../bin/acyort -c",
    "test:lint": "eslint ."
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acyortjs/acyort.git"
  },
  "keywords": [
    "website",
    "framework",
    "static"
  ],
  "author": "LoeiFy <LoeiFy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acyortjs/acyort/issues"
  },
  "homepage": "https://github.com/acyortjs/acyort",
  "dependencies": {
    "@acyort/i18n": "^0.1.1",
    "@acyort/logger": "^1.1.1",
    "@acyort/renderer": "^1.0.0",
    "fs-extra": "^7.0.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "yargs-parser": "^11.0.0"
  },
  "devDependencies": {
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  }
}
