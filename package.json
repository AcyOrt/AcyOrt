{
  "name": "acyort",
  "version": "2.0.0",
  "description": "A Node.js blog tool. Powered by GitHub",
  "bin": {
    "acyort": "./bin/acyort.js"
  },
  "preferGlobal": "true",
  "scripts": {
    "start": "cd assets && cross-env NODE_ENV=dev node ../bin/acyort server",
    "build": "cd assets && cross-env NODE_ENV=dev node ../bin/acyort build",
    "clean": "cd assets && cross-env NODE_ENV=dev node ../bin/acyort clean",
    "test": "NODE_ENV=test mocha -r intelli-espower-loader test",
    "cover": "NODE_ENV=test istanbul cover --print both _mocha -r intelli-espower-loader test",
    "test:lint": "eslint ."
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acyortjs/acyort.git"
  },
  "keywords": [
    "website",
    "blog",
    "github",
    "tool",
    "static",
    "issue"
  ],
  "author": "LoeiFy@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acyortjs/acyort/issues"
  },
  "homepage": "https://github.com/acyortjs/acyort",
  "dependencies": {
    "acyort-config": "^0.1.1",
    "acyort-extend": "^0.1.4",
    "acyort-fetcher": "^0.1.2",
    "acyort-helper": "^0.1.2",
    "acyort-logger": "^0.1.0",
    "acyort-marked": "^0.1.2",
    "acyort-processor": "^0.1.1",
    "acyort-render": "^0.1.0",
    "commander": "^2.9.0",
    "fs-extra": "^0.26.5"
  },
  "devDependencies": {
    "cross-env": "^3.1.4",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "intelli-espower-loader": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "power-assert": "^1.4.4"
  }
}
