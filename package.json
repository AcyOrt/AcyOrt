{
  "name": "acyort",
  "version": "1.5.2",
  "description": "A simple Node.js blog tool. Powered by GitHub",
  "main": "lib/acyort",
  "bin": {
    "acyort": "./bin/acyort.js"
  },
  "preferGlobal": "true",
  "scripts": {
    "start": "cd assets && cross-env NODE_ENV=dev node ../bin/acyort server",
    "build": "cd assets && cross-env NODE_ENV=dev node ../bin/acyort build",
    "clean": "cd assets && cross-env NODE_ENV=dev node ../bin/acyort clean",
    "lint": "eslint ./lib"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acyortjs/acyort.git"
  },
  "keywords": [
    "website",
    "blog",
    "github",
    "tool",
    "static",
    "issue"
  ],
  "author": "LoeiFy@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acyortjs/acyort/issues"
  },
  "homepage": "https://github.com/acyortjs/acyort",
  "dependencies": {
    "acyort-i18n": "^0.1.1",
    "axios": "^0.15.2",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "commander": "^2.9.0",
    "fs-extra": "^0.26.5",
    "highlight.js": "^9.1.0",
    "lodash": "^4.17.4",
    "markdown-toc": "^1.0.2",
    "marked": "^0.3.6",
    "mime": "^1.3.4",
    "moment": "^2.12.0",
    "moment-timezone": "^0.5.13",
    "rss": "^1.2.1",
    "swig-templates": "^2.0.2",
    "ws": "^1.1.1",
    "yamljs": "^0.2.6"
  },
  "devDependencies": {
    "cross-env": "^3.1.4",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0"
  }
}
