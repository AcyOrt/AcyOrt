{
  "name": "acyort",
  "version": "0.4.0",
  "description": "A simple Node.js blog tool. Powered by GitHub",
  "main": "lib/acyort",
  "bin": {
    "acyort": "./bin/acyort.js"
  },
  "scripts": {
    "test": "cd assets && mode=dev node ../bin/acyort server",
    "build": "cd assets && mode=dev node ../bin/acyort build",
    "clear": "cd assets && find . -maxdepth 1 -name '* *' -delete && find . -maxdepth 1 ! -name . ! -name config.yml ! -name themes -delete"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AcyOrt/acyort.git"
  },
  "keywords": [
    "website",
    "blog",
    "github",
    "tool",
    "static",
    "issue"
  ],
  "author": "LoeiFy@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AcyOrt/acyort/issues"
  },
  "homepage": "https://github.com/AcyOrt/acyort",
  "dependencies": {
    "async": "^1.5.2",
    "chokidar": "^1.6.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "fs-extra": "^0.26.5",
    "highlight.js": "^9.1.0",
    "marked": "^0.3.5",
    "mime": "^1.3.4",
    "moment": "^2.12.0",
    "pinyin": "^2.8.0",
    "probe-image-size": "^1.0.6",
    "request-promise": "^2.0.1",
    "rss": "^1.2.1",
    "swig-templates": "^2.0.2",
    "ws": "^1.1.1",
    "yamljs": "^0.2.6"
  }
}
